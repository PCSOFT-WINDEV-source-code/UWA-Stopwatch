   -
     name : Duration
     procedure_id : 1619458878283237232
     type_code : 14
     code : |1+
      //Displays a duration as an analog hand in an image control
      // Syntax:
      //Duration ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      PROCEDURE duration()
      
      nXCenter, nYCenter			are int				//Coordinates for the center of the stopwatch
      sNameTimeControl				is string = IMG_Hour..Name	//Name of the control where the hands will be displayed
      nLengthMinuteHand	is int					//Length of the minute hand
      duDuration						is duration 					//Duration (difference) to display
      
      IF gbPause THEN
      	RETURN
      END
      
      //Calculate the coordinates for the center of the stopwatch	
      nXCenter = {sNameTimeControl,indControl}..Width/2
      nYCenter = ({sNameTimeControl,indControl}..Height+20)/2 
      
      //Calculate the length of the minute hand (margin set to 30 on the smallest border)
      nLengthMinuteHand = Min({sNameTimeControl,indControl}..Width/2,{sNameTimeControl,indControl}..Height/2)-30
      
      //New drawing
      dStartDrawing(sNameTimeControl,dErase)
      
      //Time passed
      duDuration = ChronoValue(1)
      
      //Deduces the duration that must not be counted
      duDuration = duDuration - gduTimeNotCounted
      
      //Draw the hands
      DrawHours(duDuration, nLengthMinuteHand, nXCenter, nYCenter)
      DrawMinutes(duDuration, nLengthMinuteHand, nXCenter, nYCenter)
      DrawSeconds(duDuration, nLengthMinuteHand, nXCenter, nYCenter)
      
      //Display the time passed	
      EDT_Duration = DurationToString(duDuration, "HH:MM:SS")
     type : 458752
   -
     name : DrawHours
     procedure_id : 1619458878283302768
     type_code : 14
     code : |1-
      // Summary: Draws the hour hand
      // Syntax:
      //DrawHours (<duDuration>, <nLengthMinuteHand>, <nXCenter>, <nYCenter>)
      //
      // Parameters:
      //	duDuration: Duration to represent
      //	nLengthMinuteHand: Length of the longest hand, the one for the minutes
      //	nXCenter: X coordinate for the center of the stopwatch
      //	nYCenter: Y coordinate for the center of the stopwatch
      // Return value:
      // 	None
      //
      
      PROCEDURE DrawHours(duDuration, nLengthMinuteHand, nXCenter, nYCenter)
      
      nAngle					is real						//Angle for the hand
      nHandColor 		is int = DarkRed		//Color of the hand
      nLengthHourHand is int 					//Length of the hour hand
      rX, rY 					are reals					//Coordinates for the top of the hand
      
      //For the hour hand: modulo 12 and cross product of 12 in relation to 360 (360°)
      //+ 90° to go to 12h
      nAngle = modulo(duDuration..Hour,12)*360/12+90
      
      //Hour hand slightly shorter than the minute hand
      nLengthHourHand = nLengthMinuteHand-20
      
      //Calculate the coordinates for the top of the hand
      rX = nLengthHourHand*Cos(nAngle) 
      rY = nLengthHourHand*Sin(nAngle)
      
      // Draw the line for the hour hand (thickness set to 4)	
      dLine(nXCenter, nYCenter, nXCenter - rX, nYCenter - rY, nHandColor, 7)
     type : 458752
   -
     name : DrawMinutes
     procedure_id : 1619458878283368304
     type_code : 14
     code : |1-
      // Summary: Draws the minute hand
      // Syntax:
      //DrawMinutes (<duDuration>, <nLengthMinuteHand>, <nXCenter>, <nYCenter>)
      //
      // Parameters:
      //	duDuration: Duration to represent
      //	nLengthMinuteHand: Length of the longest hand, the one for the minutes
      //	nXCenter: X coordinate for the center of the stopwatch
      //	nYCenter: Y coordinate for the center of the stopwatch
      // Return value:
      // 	None
      
      PROCEDURE DrawMinutes(duDuration ,nLengthMinuteHand, nXCenter, nYCenter)
      
      nAngle					is real						//Angle for the minute hand
      nHandColor 		is int = DarkRed		//Color of the hand
      rX, rY 					are reals					//Coordinates for the top of the hand
      
      //For the minute hand: cross product of 60 in relation to 360 (360°)
      //+ 90° to go to 12h	
      nAngle = duDuration..Minute*360/60+90
      
      //Calculate the coordinates for the top of the hand
      rX = nLengthMinuteHand*Cos(nAngle) 
      rY = nLengthMinuteHand*Sin(nAngle)
      
      //Draw the line for the minute hand (thickness set to 2)	
      dLine(nXCenter , nYCenter , nXCenter - rX, nYCenter - rY , nHandColor,4)
     type : 458752
   -
     name : DrawSeconds
     procedure_id : 1619458878283433840
     type_code : 14
     code : |1-
      // Summary: Draws the second hand
      // Syntax:
      //DrawSeconds (<duDuration>, <nLengthMinuteHand>, <nXCenter>, <nYCenter>)
      //
      // Parameters:
      //	duDuration:  Duration to represent
      //	nLengthMinuteHand: Length of the longest hand, the one for the minutes
      //	nXCenter: X coordinate for the center of the stopwatch
      //	nYCenter: Y coordinate for the center of the stopwatch
      // Return value:
      // 	None
      
      
      PROCEDURE DrawSeconds(duDuration, nLengthMinuteHand, nXCenter, nYCenter)
      
      nAngle					is real						//Angle for the hand
      nHandColor 		is int = DarkRed		//Color of the hand
      rX, rY 					are reals					//Coordinates for the top of the hand
      
      
      //For the second hand: cross product of 60 in relation to 360 (360°)
      //+ 90° to go to 12h	
      nAngle = duDuration..Second*360/60+90
      
      //Calculate the coordinates for the top of the hand
      //Note: Length of the second hand identical ot the one for the minutes
      rX = nLengthMinuteHand*Cos(nAngle) 
      rY = nLengthMinuteHand*Sin(nAngle)
      
      //Draw the line for the second hand (thickness set to 1)
      dLine(nXCenter , nYCenter , nXCenter - rX, nYCenter - rY, nHandColor, 2)
